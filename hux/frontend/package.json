{
  "name": "hux-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ci": "yarn install --frozen-lockfile --silent --ignore-scripts",
    "serve": "vue-cli-service serve --port 8080",
    "serve:docker": "vue-cli-service serve --port 9090 --https --host host.docker.internal --disable-host-check",
    "build": "vue-cli-service build",
    "test": "yarn test:unit",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --no-fix",
    "lint:fix": "vue-cli-service lint --fix",
    "style": "yarn prettier --check src/",
    "style:fix": "yarn prettier --write src/",
    "prepare": "cd ../../ && husky install hux/frontend/.husky",
    "storybook:build": "vue-cli-service storybook:build -c storybook/ -o ./dist/design/",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c storybook/"
  },
  "dependencies": {
    "@okta/okta-signin-widget": "^4.5.2",
    "@okta/okta-vue": "^2.1.1",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-docs": "^6.4.13",
    "@vue/composition-api": "^1.4.9",
    "axios": "^0.26.1",
    "compression-webpack-plugin": "^9.2.0",
    "core-js": "^3.21.1",
    "d3-array": "^3.1.1",
    "d3-axis": "^3.0.0",
    "d3-chord": "^3.0.1",
    "d3-collection": "^1.0.7",
    "d3-format": "^3.1.0",
    "d3-geo": "^3.0.1",
    "d3-regression": "^1.3.9",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-time": "^3.0.0",
    "d3-transition": "^3.0.1",
    "dayjs": "^1.11.0",
    "idle-vue": "^2.0.5",
    "lodash": "^4.17.21",
    "topojson": "^3.0.2",
    "uuid": "^8.3.2",
    "vue": "^2.6.14",
    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.5.3",
    "vue-svg-loader": "^0.16.0",
    "vuetify": "^2.6.4",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@mdi/font": "^5.9.55",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/vue": "^6.4.13",
    "@types/uuid": "^8.3.4",
    "@vue/babel-preset-app": "^4.5.13",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "faker": "^5.5.3",
    "husky": "^7.0.0",
    "js-beautify": "^1.14.0",
    "lint-staged": "^11.1.2",
    "miragejs": "^0.1.41",
    "node-sass": "^7.0.1",
    "null-loader": "^4.0.1",
    "prettier": "^2.2.1",
    "sass": "~1.32.12",
    "sass-loader": "^10.0.0",
    "storybook-addon-designs": "^6.2.1",
    "vue-cli-plugin-storybook": "^2.1.0",
    "vue-cli-plugin-vuetify": "^2.4.3",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.3"
  },
  "lint-staged": {
    "src/*.{js,jsx,vue}": [
      "yarn lint",
      "yarn style"
    ]
  }
}
